.home-swiper {
  width: 750rpx;
}
.home-swiper swiper {
  width: 100%;
  height: 340rpx;
}
.home-cate {
  display: flex;
}
.home-cate navigator {
  flex: 1;
  padding: 25rpx;
}
.home-floor .floor-group .floor-title {
  padding: 10rpx 0;
}
.home-floor .floor-group .floor-title image {
  width: 100%;
}
.home-floor .floor-group .floor-list {
  overflow: hidden;
}
.home-floor .floor-group .floor-list navigator {
  float: left;
  width: 33.33%;
  /* 2 3 两个超链接 */
}
.home-floor .floor-group .floor-list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.home-floor .floor-group .floor-list navigator:nth-child(2),
.home-floor .floor-group .floor-list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.home-floor .floor-group .floor-list navigator image {
  width: 100%;
  height: 100%;
}
